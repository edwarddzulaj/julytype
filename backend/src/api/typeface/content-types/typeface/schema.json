{
  "kind": "collectionType",
  "collectionName": "typefaces",
  "info": {
    "singularName": "typeface",
    "pluralName": "typefaces",
    "displayName": "Typeface",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "aboutText": {
      "type": "richtext",
      "required": true
    },
    "specimen": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["files"]
    },
    "price": {
      "type": "float",
      "required": true
    },
    "wholePackageDiscount": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "trialFonts": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["files"]
    },
    "styles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::style.style",
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "supportedLanguages": {
      "type": "component",
      "repeatable": true,
      "component": "typeface.supported-alphabet"
    }
  }
}
